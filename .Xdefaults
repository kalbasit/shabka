!----------------------------------------------
!vim:fenc=UTF-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
!----------------------------------------------

! Xft setting
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    full
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! rxvt-unicode settings
URxvt.transparent:     false
URxvt.imLocale:        en_US.UTF-8
URxvt.scrollBar:       false
URxvt.saveLines:       32767
URxvt.cursorBlink:     true
URxvt.cursorColor:     #FF0000
!URxvt.depth:           24
URxvt.visualBall:      true
URxvt*termName:        rxvt-unicode-256color
URxvt*font:            xft:DejaVu Sans Mono:pixelsize=18:antialias=true:hinting=true
URxvt*boldFont:        xft:DejaVu Sans Mono:pixelsize=18:antialias=true:hinting=true:bold
URxvt*italicFont:      xft:DejaVu Sans Mono:pixelsize=18:antialias=true:hinting=true:italic
URxvt*bolditalicFont:  xft:DejaVu Sans Mono:pixelsize=18:antialias=true:hinting=true:bold:italic
URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher:     google-chrome
URxvt.matcher.button:  1

